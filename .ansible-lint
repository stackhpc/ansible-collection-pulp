skip_list:
  - name[missing]
  - name[play]
  - var-naming[no-reserved]
  - var-naming[no-role-prefix]
  - yaml[empty-lines]
  - yaml[indentation]
  - yaml[line-length]
  - yaml[new-line-at-end-of-file]
  - yaml[trailing-spaces]
  - yaml[truthy]
  - galaxy[no-changelog]
  - galaxy[tags]
  - galaxy[version-incorrect]
  - meta-runtime[unsupported-version]
  - fqcn[action-core]
